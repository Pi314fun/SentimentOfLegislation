		TB [label="Load and Print JSON Structure"]
		TC [label="Read and Combine JSON Files"]
		TD [label="Export to CSV"]
		TE [label=EDA]
		TA -> TB
		TB -> TC
		TC -> TD
		TD -> TE
	}
	subgraph cluster_legiscan {
		color=lightgrey fontname="Helvetica Bold" fontsize=12 label="LegiScan API Pipeline" labeljust=c style=filled
