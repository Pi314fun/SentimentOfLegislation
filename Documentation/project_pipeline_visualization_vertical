		LA -> LB
		LB -> LC
		LC -> LD
		LD -> LE
		LE -> LF
		LF -> LG
	}
	node [color=lightblue fontname=Helvetica fontsize=11 shape=box style=filled]
	edge [color=blue style=bold]
